{
  "name": "tracespace",
  "private": true,
  "scripts": {
    "_test": "mocha --require scripts/init-test-env --timeout 5000 '@(packages|apps)/**/*test.@(js|ts|tsx)'",
    "test": "cross-env INTEGRATION=1 nyc npm run _test",
    "posttest": "npm run example && npm run docs && npm run check && npm run lint",
    "test:watch": "npm run _test -- --watch --reporter dot --watch-extensions js,ts,tsx",
    "test:browser": "karma start",
    "coverage": "nyc report --reporter=lcov",
    "start": "lerna run start --parallel",
    "build": "lerna run build --parallel",
    "serve": "lerna run serve --parallel",
    "lint": "eslint '.*.js' '**/*.@(js|ts|tsx)'",
    "format": "prettier --ignore-path .eslintignore --write '.*.@(js|yml)' '**/*.@(js|ts|tsx|json|md|yml)'",
    "check": "tsc",
    "docs": "lerna run docs",
    "example": "lerna run example",
    "prebump": "npm run test",
    "bump": "lerna version",
    "ci:script": "npm run test",
    "ci:after-success": "npm run coverage && codecov",
    "ci:publish": "lerna publish from-git --yes --npm-tag $npm_package_config_publishTag"
  },
  "config": {
    "publishTag": "next",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "apps/**",
      "packages/**"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/example/**",
      "**/integration/**",
      "**/test.js",
      "**/test/**",
      "**/__tests__/**",
      "**/*.config.js",
      "packages/fixtures/**"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/enzyme": "^3.1.18",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.2.0",
    "concat-stream": "^2.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "debug": "^4.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.0.0",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "lerna": "^3.13.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^13.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pify": "^4.0.1",
    "prettier": "^1.16.4",
    "proxyquire": "^2.0.1",
    "proxyquire-universal": "^2.1.0",
    "proxyquireify": "^3.2.1",
    "pump": "^3.0.0",
    "run-parallel": "^1.1.6",
    "run-waterfall": "^1.1.3",
    "semver": "^5.6.0",
    "sinon": "^7.2.4",
    "sinon-chai": "^3.3.0",
    "snap-shot-it": "^6.2.10",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "testdouble": "^3.10.0",
    "to-readable-stream": "^1.0.0",
    "typescript": "^3.3.3",
    "watchify": "^3.11.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "worker-loader": "^2.0.0",
    "xml-formatter": "^1.0.1"
  },
  "dependencies": {
    "@tracespace/cli": "file:packages/cli",
    "@tracespace/fixtures": "file:packages/fixtures",
    "@tracespace/view": "file:apps/view",
    "@tracespace/xml-id": "file:packages/xml-id",
    "gerber-parser": "file:packages/gerber-parser",
    "gerber-plotter": "file:packages/gerber-plotter",
    "gerber-to-svg": "file:packages/gerber-to-svg",
    "pcb-stackup": "file:packages/pcb-stackup",
    "pcb-stackup-core": "file:packages/pcb-stackup-core",
    "whats-that-gerber": "file:packages/whats-that-gerber"
  }
}
