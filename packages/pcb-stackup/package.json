{
  "name": "pcb-stackup",
  "version": "0.1.0",
  "description": "Creates PCB stackup (top and bottom view) SVG renders from gerber-to-svg converters",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint lib/*.js test/*.js",
    "test": "istanbul cover -x 'integration/*' -x 'example/*' --include-all-sources _mocha",
    "posttest": "npm run lint",
    "test:watch": "mocha --reporter dot --watch",
    "pretest:integration": "browserify integration/client.js > integration/bundle.js",
    "test:integration": "node integration/server.js",
    "test:browser": "zuul --local -- ./test/*_test.js",
    "test:sauce": "zuul -- ./test/*_test.js",
    "ci": "npm test && if [ \"${TEST_BROWSERS}\" = \"true\" ]; then npm run ci:browser; fi",
    "ci:browser": "if [ \"${TRAVIS_PULL_REQUEST}\" = \"false\" ]; then npm run test:sauce; fi",
    "postci": "coveralls < ./coverage/lcov.info",
    "example": "node example/clockblock.js"
  },
  "precommit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tracespace/pcb-stackup.git"
  },
  "keywords": [
    "pcb",
    "gerber",
    "svg",
    "gerber-to-svg",
    "tracespace",
    "circuit",
    "hardware"
  ],
  "author": "Mike Cousins <mike@cousins.io> (http://cousins.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tracespace/pcb-stackup/issues"
  },
  "homepage": "https://github.com/tracespace/pcb-stackup",
  "devDependencies": {
    "async": "^1.5.2",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "domify": "^1.4.0",
    "eslint": "^2.9.0",
    "gerber-to-svg": "^1.0.0",
    "hapi": "^13.3.0",
    "inert": "^3.2.0",
    "istanbul": "^0.4.3",
    "lodash.omit": "^4.2.1",
    "lodash.template": "^4.2.4",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.4",
    "proxyquire-universal": "^1.0.8",
    "proxyquireify": "^3.2.0",
    "shortid": "^2.2.6",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "whats-that-gerber": "^1.0.0",
    "xhr": "^2.2.0",
    "zuul": "^3.10.1"
  },
  "dependencies": {
    "lodash.assign": "^4.0.8",
    "lodash.countby": "^4.3.0",
    "lodash.defaults": "^4.0.1",
    "lodash.isstring": "^4.0.1",
    "lodash.mapvalues": "^4.3.0",
    "lodash.reduce": "^4.3.0",
    "gerber-to-svg": "^1.0.0",
    "whats-that-gerber": "^1.0.0",
    "shortid": "^2.2.6",
    "whats-that-gerber": "github:tracespace/whats-that-gerber#simplify-return"
  }
}
