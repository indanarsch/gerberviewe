{
  "name": "@tracespace/xml-id",
  "publishConfig": {
    "access": "public"
  },
  "version": "5.0.0-next.0",
  "description": "XML ID generation and sanitation utilities",
  "main": "./dist/tracespace-xml-id.umd.js",
  "module": "./dist/tracespace-xml-id.es.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/tracespace-xml-id.es.js",
      "require": "./dist/tracespace-xml-id.umd.js"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "esm",
    "cjs",
    "umd"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracespace/tracespace.git"
  },
  "keywords": [
    "xml",
    "id",
    "name",
    "sanitize",
    "random"
  ],
  "contributors": [
    "Mike Cousins <mike@cousins.io> (https://mike.cousins.io)"
  ],
  "license": "MIT"
}
